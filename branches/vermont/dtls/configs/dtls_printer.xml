<ipfixConfig>
	<ipfixCollector id="1">
		<cert>exporter_cert.pem</cert>
		<key>exporter_key.pem</key>
		<CAfile>vermontCA.pem</CAfile>
		<CApath>/etc/ssl/certs</CApath>
		<listener>
			<transportProtocol>DTLS_OVER_UDP</transportProtocol>
			<port>4433</port>
			<peerFqdn>exporter.example.com</peerFqdn>
		</listener>
		<next>2</next>
	</ipfixCollector>
	<ipfixPrinter id="2">
	</ipfixPrinter>
</ipfixConfig>

